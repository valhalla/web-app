{
  "name": "converter-demo-app",
  "version": "0.0.1",
  "private": false,
  "homepage": "https://valhalla.openstreetmap.de",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:all": "yarn prettier:check && yarn lint",
    "predeploy": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx}\"",
    "prettier:check": "prettier --check \"**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx}\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx \"**/*\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@geoman-io/leaflet-geoman-free": "^2.13.1",
    "@mui/material": "^5.10.14",
    "@turf/turf": "^6.5.0",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "json-format": "^1.0.1",
    "leaflet": "^1.9.3",
    "leaflet-extra-markers": "^1.2.1",
    "leaflet.heightgraph": "^1.4.0",
    "prop-types": "^15.8.1",
    "ramda": "^0.28.0",
    "react": "^17.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.0",
    "react-modern-drawer": "^1.1.2",
    "react-redux": "^8.0.4",
    "react-toastify": "^9.1.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.3",
    "tachyons": "^4.12.0",
    "throttle-debounce": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.4.0",
    "@types/ramda": "^0.31.1",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^17.0.26",
    "@types/throttle-debounce": "^5.0.2",
    "@types/turf": "^3.5.32",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-eslint": "^10.0.2",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^3.6.2",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
