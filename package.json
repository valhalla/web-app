{
  "name": "valhalla-web",
  "version": "1.0.0",
  "private": false,
  "homepage": "https://valhalla.openstreetmap.de",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:all": "yarn prettier:check && yarn lint",
    "predeploy": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx}\"",
    "prettier:check": "prettier --check \"**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx}\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx \"**/*\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-accessible-icon": "^1.1.8",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-query": "^5.90.10",
    "@turf/turf": "^6.5.0",
    "@watergis/maplibre-gl-terradraw": "^1.8.0",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.555.0",
    "maplibre-gl": "^5.13.0",
    "next-themes": "^0.4.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-map-gl": "^8.0.4",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "throttle-debounce": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/d3": "^7.4.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^24.4.0",
    "@types/react-dom": "^18.0.0",
    "@types/throttle-debounce": "^5.0.2",
    "@types/turf": "^3.5.32",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-eslint": "^10.0.2",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^3.6.2",
    "redux-devtools-extension": "^2.13.8",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
