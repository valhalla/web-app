    "prettier:check": "prettier --check \"**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx}\"",
    "check": "npm run prettier:check && npm run lint",
    "typecheck": "tsc --noEmit",
    "deploy": "gh-pages -d build",
    "prepare": "husky",
    "check:deps": "npx taze@latest -Ilwr"
