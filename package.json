{
  "name": "valhalla-web",
  "version": "1.0.0",
  "description": "Web interface for Valhalla routing engine",
  "private": false,
  "license": "MIT",
  "homepage": "https://valhalla.openstreetmap.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/valhalla/web-app"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
